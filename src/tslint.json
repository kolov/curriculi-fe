{
  "extends": [
    "../tslint.json",
    "tslint:recommended"
  ],
  "rulesDirectory": [
    "../node_modules/tslint-eslint-rules/dist/rules"
  ],
  "rules": {
    "cyclomatic-complexity": [
      true,
      6
    ],
    "interface-name": false,
    "max-line-length": [
      true,
      140
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "no-console": [
      true,
      "debug",
      "info",
      "log",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-unused-variable": true,
    "object-curly-spacing": true,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "only-arrow-functions": false,
    "prefer-object-spread": true,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}
